/**index.wxss**/


.user-center{
  width: 100%;
  height: 50rpx;
  margin-top: 0rpx;
  overflow: hidden;
}
.user-logo{
  margin-left: 10rpx;
  margin-bottom: 10rpx;
  width: 40rpx;
  height: 40rpx;
}
.container{
 position: fixed;
  width: 100%;
  /* display:flex; */
  /* flex-direction:column; */
  /* border-bottom: 1rpx solid; */
  border-top: 1rpx solid;
  border-color: #D3D3D3;
  padding-top: 10rpx;
  /* padding-bottom: 120rpx; */
  
}
.habit-item{
  margin-left: 10rpx;
  height: 120rpx;
  border-bottom: 1rpx solid;
  border-color: #D3D3D3;
  display: flex;

}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.add-habit{
  position: absolute;
  bottom: 20rpx;
  width: 100%;
  height: 100rpx;
  text-align: center;
}
.add-image{
  height: 100%;
  width: 100rpx;
}
.finish{
  position: absolute;
  width: 120rpx;
  height: 120rpx;
  right: 10rpx;
}
.finish image{
  width: 100%;
  height: 100%;
}