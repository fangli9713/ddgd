<view class='page'>
  <view class='user-info'>
    <view class='user-image'>
      <image src='/pages/image/man_logo@128.png' style='width:100%;height:100%'></image>
    </view>
    <view class='user-phone'>
      <view>方立南</view>
      <view>18520332050</view>
    </view>
  </view>

  <view class='menu-item' style='margin-top:144rpx;' bindtap='car'>
      <image src='/pages/image/card@32.png' class='menu-item-icon'></image>
      <view class='menu-item-font'>我的车辆</view>
      <image src='/pages/image/right@16.png' class='menu-item-right'></image>

  </view>

  <view class='menu-item' bindtap='monthCard'>
      <image src='/pages/image/card@32.png' class='menu-item-icon'></image>
      <view class='menu-item-font'>我的月卡</view>
      <image src='/pages/image/right@16.png' class='menu-item-right'></image>

  </view>

  <view class='menu-item' bindtap='order'>
      <image src='/pages/image/card@32.png' class='menu-item-icon'></image>
      <view class='menu-item-font'>我的订单</view>
      <image src='/pages/image/right@16.png' class='menu-item-right'></image>

  </view>

  <view class='menu-item' bindtap='coupon'>
      <image src='/pages/image/card@32.png' class='menu-item-icon'></image>
      <view class='menu-item-font'>优惠券</view>
      <image src='/pages/image/right@16.png' class='menu-item-right'></image>


  </view>


</view>